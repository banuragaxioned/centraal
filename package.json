{
  "name": "loggrr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:web": "turbo dev --filter web",
    "start": "turbo start",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "clean": "turbo clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "ui:add": "pnpm --filter @loggrr/ui ui:add",
    "ui:diff": "pnpm --filter @loggrr/ui ui:diff",
    "manypkg:fix": "manypkg fix",
    "postinstall": "manypkg check",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@loggrr/tsconfig": "workspace:*",
    "@manypkg/cli": "^0.21.4",
    "eslint": "^8.57.0",
    "tsx": "^4.15.4",
    "turbo": "^2.0.3",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@9.3.0",
  "engines": {
    "node": "20.x"
  }
}
